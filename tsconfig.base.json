{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "strictPropertyInitialization": true,
    "jsx": "react",
  }
}