{
  "extends": "../../tsconfig.shared.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "importHelpers": false,
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es2017",
